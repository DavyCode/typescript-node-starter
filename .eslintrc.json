{
    "env": {
        "es2021": true,
        "node": true,
        "browser": true,
        "jest": true
    },
    "extends": [
        "standard",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "no-console": 1,
        "prettier/prettier": 2
    }
}